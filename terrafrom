pipeline { 
agent any 
    stages { 
         stage ('terraform init') { 
            steps {
          echo 'init'
         }
        }
         stage ('terraform plan') {
             steps {
          echo 'plane'
             }
        }
         stage ('terraform apply') {
             steps {
          echo 'apply'
             }
        }
        stage ('apply') {
             steps {
          echo 'hello'
             }
        }
    }           
 }
